{
  "env": {
    "VITE_MULTIPART_THRESHOLD_MB": "120",
    "VITE_UPLOAD_PARALLEL": "6",
    "VITE_MULTIPART_PARALLEL": "3",
    "VITE_UPLOAD_RETRY": "4",
    "VITE_UPLOAD_RETRY_BASE_MS": "1000",
    "VITE_UPLOAD_TIMEOUT_MS": "150000",
    "VITE_PRESIGN_BATCH": "60"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://metrovan-server-docker.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
